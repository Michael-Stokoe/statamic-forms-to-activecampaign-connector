{
    "name": "stokoe/forms-to-activecampaign-connector",
    "autoload": {
        "psr-4": {
            "Stokoe\\FormsToActivecampaignConnector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stokoe\\FormsToActivecampaignConnector\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2 || ^8.3",
        "statamic/cms": "^4.0 || ^5.0 || ^6.0",
        "stokoe/forms-to-wherever": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0 || ^10.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Forms To Activecampaign Connector",
            "description": "Forms To Activecampaign Connector addon"
        },
        "laravel": {
            "providers": [
                "Stokoe\\FormsToActivecampaignConnector\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
